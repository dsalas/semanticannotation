<?xml version="1.0"?>
<rdf:RDF xmlns="http://test.org/my_ontology.owl#"
         xml:base="http://test.org/my_ontology.owl"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

<owl:Ontology rdf:about="http://test.org/my_ontology.owl"/>

<owl:Class rdf:about="#Document">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Concept">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:ObjectProperty rdf:about="#documentHasConcept">
  <rdfs:domain rdf:resource="#Document"/>
  <rdfs:range rdf:resource="#Concept"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#conceptInDocument">
  <rdfs:domain rdf:resource="#Concept"/>
  <rdfs:range rdf:resource="#Document"/>
  <owl:inverseOf rdf:resource="#documentHasConcept"/>
</owl:ObjectProperty>

<Concept rdf:about="#machine">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
</Concept>

<Concept rdf:about="#learning">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
</Concept>


</rdf:RDF>
